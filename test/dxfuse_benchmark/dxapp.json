{
  "name": "dxfuse_benchmark",
  "summary": "streaming benchmark for dxfuse",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "verbose",
      "class": "boolean",
      "optional": true
    }
  ],
  "outputSpec": [
    {
      "name": "result",
      "class": "array:string"
    },
    {
      "name": "result_symlinks",
      "class": "array:string"
      "optional" : true,
    }
  ],
  "runSpec": {
    "timeoutPolicy" : {
      "*" : {
        "hours" : 1
      }
    },
    "interpreter": "bash",
    "file": "code.sh",
    "distribution": "Ubuntu",
    "release": "16.04"
  }
}
