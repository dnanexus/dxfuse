{
  "name": "benchmark",
  "summary": "streaming benchmark for dxfuse",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "verbose",
      "class": "boolean",
      "optional": true
    }
  ],
  "outputSpec": [
    {
      "name": "result",
      "class": "array:string",
      "optional" : true
    },
    {
      "name": "result_symlinks",
      "class": "array:string",
      "optional" : true
    },
    {
      "name": "result_upload",
      "class": "array:string",
      "optional" : true
    }
  ],
  "runSpec": {
    "systemRequirements": {
      "*":{
        "instanceType": "mem3_hdd2_v2_x8"
      }
    },
    "timeoutPolicy" : {
      "*" : {
        "hours" : 2
      }
    },
    "interpreter": "bash",
    "file": "code.sh",
    "distribution": "Ubuntu",
    "release": "20.04",
    "version": "0"
  },
  "access": {
    "project" : "CONTRIBUTE",
    "network" : [ "*" ]
  }
}
